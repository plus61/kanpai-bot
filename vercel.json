{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "crons": [
    {
      "path": "/cron/dm-timeout",
      "schedule": "* * * * *"
    },
    {
      "path": "/cron/vote-timeout",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/cron/monitor",
      "schedule": "*/30 * * * *"
    }
  ]
}
